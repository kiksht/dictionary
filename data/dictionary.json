  "wi-mał": {
    "root": "wi-mał",
    "partOfSpeech": "n",
    "definition": "river, Columbia River",
    "forms": [
      {
        "kiksht": "wímałmax̲",
        "english": "rivers"
      }
    ],
    "examples": [
      {
        "kiksht": "Galułx̲a wimałiamt.",
        "english": "She went to the river."
      },
      {
        "kiksht": "yaka wiimał",
        "english": "all along the river [Dyk's transcription: yaka we:mał]"
      },
      {
        "kiksht": "[Dyk]   G̲angadix galux̲wílaitix adasq'ú idelxam kana'amdmax̲ wimałba.",
        "english": "Long ago the Wasco people lived on both sides of the Columbia River."
      }
    ],
    "notes": "Comment by Phillip Kahclamat: \"Sapir said the Columbia is the only one called wimał, but I'd call the Amazon, Mississippi that.\" Apparently this word refers to large rivers. Compare wi-qał.",
    "seeAlso": [
      "id-mał, małn, k'amałmt, it-k'umałmt"
    ]
  },
